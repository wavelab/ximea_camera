<launch>

<!-- Camera Node -->
<node name="ximea" pkg="ximea_camera" type="ximea_camera_node" output="screen" >
  <param name="frame_rate" type="int" value="30" />
  
  <rosparam param="camera_param_file_paths" subst_value="true">[$(find ximea_camera)/config/camera2_dyn_reconfigure.yaml]</rosparam>
  <param name="config_directory" type="string" value="$(find ximea_camera)/config/" />
  
</node>

</launch>
